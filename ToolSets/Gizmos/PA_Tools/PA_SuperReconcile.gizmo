 addUserKnob {22 create l Create -STARTLINE T "#SUPER RECONCILE\n#CREATED BY PEDRO ANDRADE\n#-2019-\n\n\nwith nuke.thisNode():\n\n#CALCULATE\n    if nuke.thisNode().knob('auxiliar_method').value() == 'Axis/Card':\n        nuke.thisNode().knob('reset').execute()\n        nuke.toNode('Transformed_2D_Coordinates').knob('calculate_all').execute()\n\n#FORCED SET\n    if nuke.thisNode().knob('input_type').value() == 'Plate':\n        nuke.thisNode().knob('set').execute()\n    else:\n        nuke.thisNode().knob('set_2').execute()\n\n#FORCED AXIS RESET\n    if not nuke.thisNode().knob('selection_mode').value() == 'Quad':\n        for n in range(1, 4+1):\n            nuke.toNode('CornerPin2D1').knob('axis' + str(n)).setValue(0)\n            nuke.toNode('Tracker1').knob('axis' + str(n)).setValue(0)\n    \n\n#EXPORTS\n    if nuke.thisNode().knob('export').value() == 'CornerPin':\n        for n in nuke.allNodes():\n            n.knob('selected').setValue(False)\n\n        nuke.toNode('CornerPin2D1').knob('selected').setValue(True)\n\n        for n in nuke.allNodes('CornerPin2D'):\n            if n.name().startswith('CornerPin2D1'):\n                n.knob('current_frame').execute()\n                nuke.toNode('Crop_CornerPin').knob('selected').setValue(True)\n                nuke.nodeCopy('%clipboard%')\n\n    else:\n        for n in nuke.allNodes():\n            n.knob('selected').setValue(False)\n\n            nuke.toNode('Tracker1').knob('selected').setValue(True)            \n\n        for n in nuke.allNodes('Tracker3'):\n            n.knob('reference_frame').setValue(nuke.frame())\n            nuke.toNode('Crop_Tracker').knob('selected').setValue(True)\n            nuke.nodeCopy('%clipboard%')\n\n    for n in nuke.allNodes():\n        n.knob('selected').setValue(False)\n\nnuke.nodePaste('%clipboard%')\n\nlabel = str(nuke.getInput('Add Label', 'Add Label'))\nfor n in nuke.selectedNodes():\n    if not n.Class() == 'Crop':\n        n.knob('label').setValue(label.upper() + '\\n\\n'+n.knob('label').value())\ntry:\n    nuke.extractSelected()\nexcept:\n    pass\n\nfor n in nuke.allNodes():\n    n.knob('selected').setValue(False)\n\n\n\n"}
