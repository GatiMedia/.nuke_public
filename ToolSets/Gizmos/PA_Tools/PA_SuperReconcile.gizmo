 name PA_SuperReconcile
 knobChanged "\n\n\n#SUPER RECONCILE\n#CREATED BY PEDRO ANDRADE\n#-2019-\n\nwith nuke.thisNode():\n\n\n    #TEST MODE\n    if nuke.thisKnob().name() == 'test_mode':\n        nuke.toNode('CornerPin2D3').knob('ref_frame').setValue(nuke.frame())\n\n    if nuke.thisNode().knob('input_type').value() == 'Plate':\n\n        nuke.thisNode().knobs()\['in'].setVisible(False)\n        nuke.thisNode().knobs()\['in_1'].setVisible(False)\n\n        nuke.thisNode().knob('place_at').setVisible(False)\n        nuke.thisNode().knob('set').setVisible(False)\n        nuke.thisNode().knob('auxiliar_method').setVisible(True)\n        nuke.thisNode().knob('auxiliar_method_2').setVisible(False)\n\n       \n        try:\n\n            for n in nuke.allNodes('Input'):\n                if n.knob('name').value() == 'position_pass':\n                    auxInputXPos = n.knob('xpos').value()\n                    auxInputYPos = n.knob('ypos').value()\n                    nuke.nodes.StickyNote(name = '_StickyNote_1', label = str(auxInputXPos) + ',' + str(auxInputYPos))\n                    nuke.delete(n)\n                                  \n\n            dotAux = nuke.nodes.Input(name='aux', xpos = nuke.toNode('_StickyNote_').knob('label').value().split(',')\[0], ypos = nuke.toNode('_StickyNote_').knob('label').value().split(',')\[1])\n            nuke.delete(nuke.toNode('_StickyNote_'))\n            for n in nuke.allNodes('Dot'):\n                if n.knob('label').value() == 'aux_input':\n                    n.setInput(0, dotAux)\n\n        except:\n            pass\n\n        if nuke.thisNode().knob('auxiliar_method').value() == 'Geo/Cones':\n            nuke.thisNode().knob('place_at').setVisible(True)\n            nuke.thisNode().knob('set').setVisible(True)\n            nuke.thisNode().knob('set_2').setVisible(False)\n\n        else:\n            nuke.thisNode().knob('place_at').setVisible(False)\n            nuke.thisNode().knob('set').setVisible(False)\n            nuke.thisNode().knob('set_2').setVisible(False)\n    \n    \n    else:\n\n        nuke.thisNode().knobs()\['in'].setVisible(True)\n        nuke.thisNode().knobs()\['in_1'].setVisible(True)\n\n        nuke.thisNode().knob('place_at').setVisible(True)\n        nuke.thisNode().knob('set').setVisible(False)\n        nuke.thisNode().knob('set_2').setVisible(True)\n    \n        nuke.thisNode().knob('auxiliar_method').setVisible(False)\n        nuke.thisNode().knob('auxiliar_method_2').setVisible(True)\n\n        nuke.toNode('PositionPass2').knob('in').setEnabled(True)\n\n        nuke.thisNode().knob('auxiliar_method').setValue('Geo/Cones')\n\n        try:   \n\n            for n in nuke.allNodes('Input'):\n                if n.knob('name').value() == 'aux':\n                    auxInputXPos = n.knob('xpos').value()\n                    auxInputYPos = n.knob('ypos').value()\n                    nuke.nodes.StickyNote(name = '_StickyNote_', label = str(auxInputXPos) + ',' + str(auxInputYPos))\n                    nuke.delete(n)\n\n                                        \n        except:\n            pass\n\n\n    #MAP TO SURFACE\n    if nuke.thisNode().knob('map_surface').value() == True:\n        nuke.thisNode().knob('look_cam').setEnabled(False)\n        nuke.thisNode().knob('face_camera').setEnabled(False)\n    else:\n        nuke.thisNode().knob('look_cam').setEnabled(True)\n        nuke.thisNode().knob('face_camera').setEnabled(True)\n\n    if nuke.thisKnob().name() == 'map_surface':\n        nuke.thisNode().knob('xtra_translate').setValue(0)\n        nuke.thisNode().knob('xtra_rotation').setValue(0)\n        nuke.thisNode().knob('xtra_scaling').setValue(0)\n\n    \n    #LOOK AT CAM  \n    if nuke.thisNode().knob('look_cam').value() == True:\n\n        nuke.thisNode().knob('xtra_translate').setValue(0)\n        nuke.thisNode().knob('xtra_rotation').setValue(0)\n        nuke.thisNode().knob('xtra_scaling').setValue(0)\n\n        nuke.thisNode().knob('xtra_translate').setEnabled(False)\n        nuke.thisNode().knob('xtra_rotation').setEnabled(False)\n        nuke.thisNode().knob('xtra_scaling').setEnabled(False)\n        nuke.thisNode().knob('map_surface').setEnabled(False)\n        nuke.thisNode().knob('face_camera').setEnabled(False)\n\n\n    else:\n        nuke.thisNode().knob('xtra_translate').setEnabled(True)\n        nuke.thisNode().knob('xtra_rotation').setEnabled(True)\n        nuke.thisNode().knob('xtra_scaling').setEnabled(True)\n        nuke.thisNode().knob('map_surface').setEnabled(True)\n        nuke.thisNode().knob('face_camera').setEnabled(True)\n\n    if nuke.thisNode().knob('map_surface').value() == True or nuke.thisNode().knob('look_cam').value() == True:\n        nuke.thisNode().knob('face_camera').setEnabled(False)\n    else:\n        nuke.thisNode().knob('face_camera').setEnabled(True)\n\n\n\n    #SELECTION MODE\n    if nuke.thisNode().knob('selection_mode').value() == 'Quad':\n\n        for n in range(1, 4+1):\n            nuke.thisNode().knobs()\['to'+str(n)].setVisible(True)\n            nuke.thisNode().knob('place_at').setVisible(False)\n            \n            nuke.toNode('CornerPin2D2').knob('to'+str(n)).clearAnimated()\n            nuke.toNode('CornerPin2D2').knob('from'+str(n)).clearAnimated()\n\n\n\n    else:\n      \n        for n in range(1, 4+1):\n            nuke.thisNode().knobs()\['to'+str(n)].setVisible(False)\n            nuke.thisNode().knob('place_at').setVisible(True)\n            \n            nuke.toNode('CornerPin2D2').knob('to'+str(n)).setValue(nuke.toNode('Transformed_2D_Coordinates').knob('point'+str(n)).value())\n            nuke.toNode('CornerPin2D2').knob('from'+str(n)).setValue(nuke.toNode('Transformed_2D_Coordinates').knob('point'+str(n)).value())\n  \n\n\n    if not nuke.thisNode().knob('auxiliar_method').value() == 'Geo/Cones':\n        nuke.thisNode().knob('selection_mode').setVisible(False)\n        nuke.thisNode().knob('place_at').setVisible(False)\n        nuke.thisNode().knob('set').setVisible(False)\n        nuke.thisNode().knob('set_2').setVisible(False)\n\n    else:\n        nuke.thisNode().knob('selection_mode').setVisible(True)\n\n\n    if nuke.thisNode().knob('auxiliar_method').value() == 'Axis/Card' or nuke.thisNode().knob('selection_mode').value() == 'Quad':\n        nuke.thisNode().knob('xtra_translate').setValue(0)\n        nuke.thisNode().knob('xtra_rotation').setValue(0)\n        nuke.thisNode().knob('xtra_scaling').setValue(0)\n        nuke.thisNode().knob('xtra_uniform_scale').setValue(1)\n        nuke.thisNode().knob('map_surface').setValue(False)\n        nuke.thisNode().knob('look_cam').setValue(False)\n\n        nuke.thisNode().knobs()\['additional_transforms'].setVisible(False)\n        nuke.thisNode().knobs()\['divider_line1'].setVisible(False)\n        nuke.thisNode().knobs()\['divider_line2'].setVisible(False)\n        nuke.thisNode().knobs()\['divider_line_trs'].setVisible(False)\n        nuke.thisNode().knob('map_surface').setVisible(False)\n        nuke.thisNode().knob('face_camera').setVisible(False)\n        nuke.thisNode().knob('xtra_uniform_scale').setVisible(False)\n        nuke.thisNode().knob('face_camera').setVisible(False)\n        nuke.thisNode().knob('map_surface').setVisible(False)\n        nuke.thisNode().knob('look_cam').setVisible(False)\n\n    else:\n        nuke.thisNode().knobs()\['additional_transforms'].setVisible(True)\n        nuke.thisNode().knobs()\['divider_line1'].setVisible(True)\n        nuke.thisNode().knobs()\['divider_line2'].setVisible(True)\n        nuke.thisNode().knobs()\['divider_line_trs'].setVisible(True)\n        nuke.thisNode().knob('map_surface').setVisible(True)\n        nuke.thisNode().knob('face_camera').setVisible(True)\n        nuke.thisNode().knob('xtra_uniform_scale').setVisible(True)\n        nuke.thisNode().knob('face_camera').setVisible(True)\n        nuke.thisNode().knob('map_surface').setVisible(True)\n        nuke.thisNode().knob('look_cam').setVisible(True)\n\n\n\n    if nuke.thisNode().knob('auxiliar_method').value() == 'Axis/Card' and not nuke.thisNode().knob('selection_mode').value() == 'Quad':\n        nuke.thisNode().knob('reset').setVisible(False)\n    else:\n        nuke.thisNode().knob('reset').setVisible(True)\n        nuke.thisNode().knobs()\['divider_line2'].setVisible(True)\n\n\n    if nuke.thisNode().knob('input_type').value() == 'CG' and nuke.thisNode().knob('selection_mode').value() == 'Quad':\n        nuke.thisNode().knobs()\['in'].setVisible(False)\n        nuke.thisNode().knobs()\['in_1'].setVisible(False)\n\n    elif nuke.thisNode().knob('input_type').value() == 'Plate' or nuke.thisNode().knob('selection_mode').value() == 'Quad':\n        nuke.thisNode().knobs()\['in'].setVisible(False)\n        nuke.thisNode().knobs()\['in_1'].setVisible(False)\n\n    else:\n        nuke.thisNode().knobs()\['in'].setVisible(True)\n        nuke.thisNode().knobs()\['in_1'].setVisible(True)\n\n\n    #SET COLOR    \n    knobsList = list(nuke.thisNode().knobs())\n    knobsList.remove('set')\n    knobsList.remove('set_2')\n    knobsList.remove('selected')\n    knobsList.remove('test_mode')\n\n    for n in knobsList:\n        if nuke.thisKnob().name() == n:\n            nuke.toNode('Switch10').knob('which').setValue(0)\n            nuke.toNode('CornerPin2D2').knob('gl_color').setValue(4278190335)\n\n\n"
