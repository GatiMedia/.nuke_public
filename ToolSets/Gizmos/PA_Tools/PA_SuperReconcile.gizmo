 NoOp {
  name Transformed_2D_Coordinates
  tile_color 0xffff00ff
  note_font "Verdana Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold Bold"
  xpos 100
  ypos 491
  addUserKnob {20 User}
  addUserKnob {12 point1 l Point1}
  addUserKnob {12 point2 l Point2}
  addUserKnob {12 point3 l Point3}
  addUserKnob {12 point4 l Point4}
  addUserKnob {22 calculate_all T "import nuke\nfrom nukescripts import snap3d\nimport _nukemath\nimport math\n\n#SUPER RECONCILE\n#CREATED BY PEDRO ANDRADE\n#-2019-\n\n\n#GET PROJECTION MATRIX IN ALL FRAMES\n###############################################################\n#   Note: this is mostly the same function provided by The Foundry      \n#   in nukescripts.snap3d.cameraProjectionMatrix(), with only                 \n#   a few modifications to accept a \"frame\" argument                      \n##############################################################\n\n\ndef cameraProjectionMatrixAt(cameraNode, frame):\n    #Calculate the projection matrix for the camera, based on it's knob values\n    \n    # Matrix to transform points into camera-relative coords.\n    ctx = nuke.OutputContext()\n    ctx.setFrame(frame)\n    camTransform = cameraNode\['transform'].value(ctx).inverse()\n    \n    # Matrix to take the camera projection knobs into account\n    roll = float(cameraNode\['winroll'].getValueAt(frame,0))\n    scale_x = float(cameraNode\['win_scale'].getValueAt(frame,0))\n    scale_y = float(cameraNode\['win_scale'].getValueAt(frame,1))\n    translate_x = float(cameraNode\['win_translate'].getValueAt(frame,0))\n    translate_y = float(cameraNode\['win_translate'].getValueAt(frame,1))\n    m = _nukemath.Matrix4()\n    m.makeIdentity()\n    m.rotateZ(math.radians(roll))\n    m.scale(1.0 / scale_x, 1.0 / scale_y, 1.0)\n    m.translate(-translate_x, -translate_y, 0.0)\n    \n    # Projection matrix based on the focal length, aperture and clipping planes of the camera\n    focal_length = float(cameraNode\['focal'].getValueAt(frame))\n    h_aperture = float(cameraNode\['haperture'].getValueAt(frame))\n    near = float(cameraNode\['near'].getValueAt(frame))\n    far = float(cameraNode\['far'].getValueAt(frame))\n    projection_mode = int(cameraNode\['projection_mode'].getValueAt(frame))\n    p = _nukemath.Matrix4()\n    p.projection(focal_length / h_aperture, near, far, projection_mode == 0)\n    \n    # Matrix to translate the projected points into normalised pixel coords\n    format = nuke.root()\['format'].value()\n    imageAspect = float(format.height()) / float(format.width())\n    t = _nukemath.Matrix4()\n    t.makeIdentity()\n    t.translate( 1.0, 1.0 - (1.0 - imageAspect / float(format.pixelAspect())), 0.0 )\n    \n    # Matrix to scale normalised pixel coords into actual pixel coords.\n    x_scale = float(format.width()) / 2.0\n    y_scale = x_scale * format.pixelAspect()\n    s = _nukemath.Matrix4()\n    s.makeIdentity()\n    s.scale(x_scale, y_scale, 1.0)\n    \n    # The projection matrix transforms points into camera coords, modifies based\n    # on the camera knob values, projects points into clip coords, translates the\n    # clip coords so that they lie in the range 0,0 - 2,2 instead of -1,-1 - 1,1,\n    # then scales the clip coords to proper pixel coords.\n    return s * t * p * m * camTransform\n\n############################################################################\n\n\ndef calculate():\n\n    #EXTRACTING POINTS(VECTORS)\n    nuke.toNode('VectorsExtractor').knob('reset_all').execute()\n    nuke.toNode('VectorsExtractor').knob('extract_all').execute()\n\n\n    #CHANGING ROOT FORMAT\n    nuke.root().knob('format').setValue(nuke.addFormat(nuke.tcl('value \[topnode parent.input0].format')))\n\n    curFrame = nuke.frame()\n    for n in range(1, 4+1):\n        point = 'point' + str(n)\n        tracks = 'tracks.' + str(n)\n        cornerPinTo = 'to' + str(n)\n        \n        nuke.toNode('Transformed_2D_Coordinates').knob(point).clearAnimated()\n        nuke.toNode('Transformed_2D_Coordinates').knob(point).setValue(0)\n\n        pointVector_x = float(nuke.toNode('VectorsExtractor').knob(point).value()\[0])\n        pointVector_y = float(nuke.toNode('VectorsExtractor').knob(point).value()\[1])\n        pointVector_z = float(nuke.toNode('VectorsExtractor').knob(point).value()\[2])\n        pointVector_w = float(1.0)\n\n        vectorToTransform = nuke.math.Vector4(pointVector_x, pointVector_y, pointVector_z, pointVector_w)\n\n        #2D POINT CONVERSION\n        firstFrame = int(nuke.root().knob('first_frame').value())\n        lastFrame = int(nuke.root().knob('last_frame').value())\n\n        transformedVectorReconciledList_x = \[]\n        transformedVectorReconciledList_y = \[]\n        for n in range(firstFrame, lastFrame+1):\n            transformedVector = cameraProjectionMatrixAt(nuke.toNode('CameraMaster'), n) * vectorToTransform\n\n    #2D POINT CONVERTED      \n            transformedVectorReconciled = nuke.math.Vector2(transformedVector.x/transformedVector.w, transformedVector.y/transformedVector.w)\n\n            transformedVectorReconciledList_x.append(transformedVectorReconciled\[0])\n            transformedVectorReconciledList_y.append(transformedVectorReconciled\[1])\n\n    #2D POINT CONVERTED VECTOR TO ANIMATION OBJECT\n        pointsXKeys = \[]\n        pointsYKeys = \[]\n        curFrame = nuke.frame()\n        for n in range(firstFrame, lastFrame+1):\n            pointsX = nuke.AnimationKey(n, float(transformedVectorReconciledList_x\[n-firstFrame]))\n            pointsY = nuke.AnimationKey(n, float(transformedVectorReconciledList_y\[n-firstFrame]))\n\n            pointsXKeys.append(pointsX)\n            pointsYKeys.append(pointsY)\n\n        nuke.toNode('Transformed_2D_Coordinates').knob(point).setAnimated(0, 0)\n        nuke.toNode('Transformed_2D_Coordinates').knob(point).animation(0).addKey(pointsXKeys)\n\n        nuke.toNode('Transformed_2D_Coordinates').knob(point).setAnimated(1, 0)\n        nuke.toNode('Transformed_2D_Coordinates').knob(point).animation(1).addKey(pointsYKeys)\n\n\n    #COPYING ANIMATION TO TRACKER AND CORNER PIN\n        #TRACKER\n        nuke.toNode('Tracker1').knob('track1').copyAnimations(nuke.toNode('Transformed_2D_Coordinates').knob('point1').animations())\n        nuke.toNode('Tracker1').knob('track2').copyAnimations(nuke.toNode('Transformed_2D_Coordinates').knob('point2').animations())\n        nuke.toNode('Tracker1').knob('track3').copyAnimations(nuke.toNode('Transformed_2D_Coordinates').knob('point3').animations())\n        nuke.toNode('Tracker1').knob('track4').copyAnimations(nuke.toNode('Transformed_2D_Coordinates').knob('point4').animations())\n\n        nuke.toNode('Tracker1').knob('translate_1').setValue(nuke.toNode('TransformGeo1').knob('translate').value())\n        nuke.toNode('Tracker1').knob('rotate_1').setValue(nuke.toNode('TransformGeo1').knob('rotate').value())\n        nuke.toNode('Tracker1').knob('scale_1').setValue(nuke.toNode('TransformGeo1').knob('scaling').value())\n        nuke.toNode('Tracker1').knob('uniform_scale').setValue(nuke.toNode('TransformGeo1').knob('uniform_scale').value())\n        nuke.toNode('Tracker1').knob('orientation').setValue(nuke.toNode('Axis1').knob('translate').value())\n        nuke.toNode('Tracker1').knob('lock_facing_camera').setValue(nuke.toNode('Switch1').knob('which').value())\n\n        nuke.toNode('Crop_Tracker').knob('box').setValue(nuke.toNode('Crop_Input').knob('box').value())\n\n        nuke.toNode('Tracker1').knob('translate_1').setEnabled(False)\n        nuke.toNode('Tracker1').knob('rotate_1').setEnabled(False)\n        nuke.toNode('Tracker1').knob('scale_1').setEnabled(False)\n        nuke.toNode('Tracker1').knob('uniform_scale').setEnabled(False)\n        nuke.toNode('Tracker1').knob('orientation').setEnabled(False)\n\n        nuke.toNode('Tracker1').knob('axis1').setEnabled(False)\n        nuke.toNode('Tracker1').knob('axis2').setEnabled(False)\n        nuke.toNode('Tracker1').knob('axis3').setEnabled(False)\n        nuke.toNode('Tracker1').knob('axis4').setEnabled(False)\n\n        if nuke.toNode('Switch7').knob('which').value() == 1:\n            nuke.toNode('Tracker1').knob('axis1').setValue(nuke.toNode('VectorsExtractor').knob('point1').value())\n            nuke.toNode('Tracker1').knob('axis2').setValue(nuke.toNode('VectorsExtractor').knob('point2').value())\n            nuke.toNode('Tracker1').knob('axis3').setValue(nuke.toNode('VectorsExtractor').knob('point3').value())\n            nuke.toNode('Tracker1').knob('axis4').setValue(nuke.toNode('VectorsExtractor').knob('point4').value())\n\n        else:\n            pass\n\n\n        #CORNER PIN\n        nuke.toNode('CornerPin2D1').knob('to1').copyAnimations(nuke.toNode('Transformed_2D_Coordinates').knob('point1').animations())\n        nuke.toNode('CornerPin2D1').knob('to2').copyAnimations(nuke.toNode('Transformed_2D_Coordinates').knob('point2').animations())\n        nuke.toNode('CornerPin2D1').knob('to3').copyAnimations(nuke.toNode('Transformed_2D_Coordinates').knob('point3').animations())\n        nuke.toNode('CornerPin2D1').knob('to4').copyAnimations(nuke.toNode('Transformed_2D_Coordinates').knob('point4').animations())\n\n        nuke.toNode('CornerPin2D1').knob('translate').setValue(nuke.toNode('TransformGeo1').knob('translate').value())\n        nuke.toNode('CornerPin2D1').knob('rotate').setValue(nuke.toNode('TransformGeo1').knob('rotate').value())\n        nuke.toNode('CornerPin2D1').knob('scale').setValue(nuke.toNode('TransformGeo1').knob('scaling').value())\n        nuke.toNode('CornerPin2D1').knob('uniform_scale').setValue(nuke.toNode('TransformGeo1').knob('uniform_scale').value())\n        nuke.toNode('CornerPin2D1').knob('orientation').setValue(nuke.toNode('Axis1').knob('translate').value())\n        nuke.toNode('CornerPin2D1').knob('lock_facing_camera').setValue(nuke.toNode('Switch1').knob('which').value())\n\n        nuke.toNode('Crop_CornerPin').knob('box').setValue(nuke.toNode('Crop_Input').knob('box').value())\n\n        nuke.toNode('CornerPin2D1').knob('translate').setEnabled(False)\n        nuke.toNode('CornerPin2D1').knob('rotate').setEnabled(False)\n        nuke.toNode('CornerPin2D1').knob('scale').setEnabled(False)\n        nuke.toNode('CornerPin2D1').knob('uniform_scale').setEnabled(False)\n        nuke.toNode('CornerPin2D1').knob('orientation').setEnabled(False)\n\n        nuke.toNode('CornerPin2D1').knob('axis1').setEnabled(False)\n        nuke.toNode('CornerPin2D1').knob('axis2').setEnabled(False)\n        nuke.toNode('CornerPin2D1').knob('axis3').setEnabled(False)\n        nuke.toNode('CornerPin2D1').knob('axis4').setEnabled(False)\n\n        if nuke.toNode('Switch7').knob('which').value() == 1:\n            nuke.toNode('CornerPin2D1').knob('axis1').setValue(nuke.toNode('VectorsExtractor').knob('point1').value())\n            nuke.toNode('CornerPin2D1').knob('axis2').setValue(nuke.toNode('VectorsExtractor').knob('point2').value())\n            nuke.toNode('CornerPin2D1').knob('axis3').setValue(nuke.toNode('VectorsExtractor').knob('point3').value())\n            nuke.toNode('CornerPin2D1').knob('axis4').setValue(nuke.toNode('VectorsExtractor').knob('point4').value())\n        else:\n            pass\n\n\n\n    #FLAGGING VECTORS NOT EXTRATED\n    if nuke.toNode('VectorsExtractor').knob('point1').value() == \[0.0, 0.0, 0.0] and \\\n        nuke.toNode('VectorsExtractor').knob('point2').value() == \[0.0, 0.0, 0.0] and \\\n        nuke.toNode('VectorsExtractor').knob('point3').value() == \[0.0, 0.0, 0.0] and \\\n        nuke.toNode('VectorsExtractor').knob('point4').value() == \[0.0, 0.0, 0.0]:\n\n        nuke.message('Something went wrong. Please reposition and re-set the region again')        \n\n\n    #HACK\n    nuke.frame(curFrame+1)\n    nuke.frame(curFrame-1+1)\n\n\n#CALCULATIONS\nnuke.clearDiskCache()\nnuke.clearRAMCache()\n\nrootFormat = nuke.tcl('format \[value root.format]')\n\ncalculate()\n\n\n#REVERT BACK TO ORIGINAL ROOT FORMAT\nnuke.root().knob('format').setValue(nuke.addFormat(rootFormat))\n\n    \n" +STARTLINE}
  addUserKnob {22 reset_all -STARTLINE T "#SUPER RECONCILE\n#CREATED BY PEDRO ANDRADE\n#-2019-\n\n\nfor n in range(1, 4+1):\n    point = 'point' + str(n)\n    \n    nuke.toNode('Transformed_2D_Coordinates').knob(point).clearAnimated()\n    nuke.toNode('Transformed_2D_Coordinates').knob(point).setValue(0)\n\n\nnuke.toNode('VectorsExtractor').knob('reset_all').execute()\n\n\n#CORNER PIN\nnuke.toNode('CornerPin2D1').knob('to1').clearAnimated()\nnuke.toNode('CornerPin2D1').knob('to2').clearAnimated()\nnuke.toNode('CornerPin2D1').knob('to3').clearAnimated()\nnuke.toNode('CornerPin2D1').knob('to4').clearAnimated()\n\nnuke.toNode('CornerPin2D1').knob('to1').setValue(0)\nnuke.toNode('CornerPin2D1').knob('to2').setValue(0)\nnuke.toNode('CornerPin2D1').knob('to3').setValue(0)\nnuke.toNode('CornerPin2D1').knob('to4').setValue(0)\n\n\nnuke.toNode('CornerPin2D1').knob('translate').setValue(0)\nnuke.toNode('CornerPin2D1').knob('rotate').setValue(0)\nnuke.toNode('CornerPin2D1').knob('scale').setValue(1)\nnuke.toNode('CornerPin2D1').knob('uniform_scale').setValue(1)\nnuke.toNode('CornerPin2D1').knob('orientation').setValue(0)\nfor n in range(1, 4+1):\n    nuke.toNode('CornerPin2D1').knob('axis' + str(n)).setValue(0)\n\n\n\n#TRACKER\nnuke.toNode('Tracker1').knob('track1').clearAnimated()\nnuke.toNode('Tracker1').knob('track2').clearAnimated()\nnuke.toNode('Tracker1').knob('track3').clearAnimated()\nnuke.toNode('Tracker1').knob('track4').clearAnimated()\n\nnuke.toNode('Tracker1').knob('track1').setValue(0)\nnuke.toNode('Tracker1').knob('track2').setValue(0)\nnuke.toNode('Tracker1').knob('track3').setValue(0)\nnuke.toNode('Tracker1').knob('track4').setValue(0)\n\nnuke.toNode('Tracker1').knob('translate_1').setValue(0)\nnuke.toNode('Tracker1').knob('rotate_1').setValue(0)\nnuke.toNode('Tracker1').knob('scale_1').setValue(1)\nnuke.toNode('Tracker1').knob('uniform_scale').setValue(1)\nnuke.toNode('Tracker1').knob('orientation').setValue(0)\nfor n in range(1, 4+1):\n    nuke.toNode('Tracker1').knob('axis' + str(n)).setValue(0)\n\n"}
